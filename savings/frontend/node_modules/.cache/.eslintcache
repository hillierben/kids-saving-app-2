,"import React from 'react';\nimport './App.css';\nimport { Route, Routes } from 'react-router-dom';\nimport Dashboard from'./components/Dashboard';\nimport Login from './components/Login';\nimport Navbar from './components/Navbar';\nimport ParentPortal from './components/ParentPortal';\nimport Register from './components/Register';\nimport ErrorPage from './components/ErrorPage';\nimport { BrowserRouter } from 'react-router-dom';\nimport TokenContext from './contexts/Token';\n\nfunction App() {\n  const[pageSelector, setPageSelector] = React.useState(\"login\");\n  const[page, setPage] = React.useState(<Login />);\n  const[loggedOut, setLoggedOut] = React.useState(\"\")\n  const[token, setToken] = React.useState('')\n  const[user, setUser] = React.useState()\n\n  return (\n    <TokenContext.Provider value={{token, setToken, user, setUser}}>\n      <BrowserRouter>\n        <Routes>\n          <Route path=\"/\" element={<Navbar \n            handlePage={e => setPageSelector(e)}\n            setLoggedOut={e => setLoggedOut(e)}/>}>\n              <Route path=\"/register\" element={<Register />} />\n              <Route path=\"/login\" element={<Login setPageSelector={e => setPageSelector(e)}/>} />\n              <Route path=\"/dashboard\" element={<Dashboard />} />\n              <Route path=\"/portal\" element={<ParentPortal />} />\n          </Route>\n          <Route path=\"*\" element={<ErrorPage />} />\n        </Routes>\n      </BrowserRouter>\n    </TokenContext.Provider>\n  );\n}\n\nexport default App;\n","/Users/benhillier/Documents/GitHub/kids-saving-app/savings/frontend/src/components/Register.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app/savings/frontend/src/components/Dashboard.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app/savings/frontend/src/components/ErrorPage.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app/savings/frontend/src/components/Navbar.js",["206","207"],[],"import React from \"react\";\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome'\nimport { faBars } from '@fortawesome/free-solid-svg-icons'\nimport MenuListData from \"./MenuListData\";\nimport Sidebar from \"./Sidebar\";\nimport { Outlet } from \"react-router-dom\";\nimport { Navigate } from \"react-router-dom\";\nimport { useContext } from \"react\";\nimport TokenContext from \"../contexts/Token\";\n\nconst Navbar = (props) => {\n    const[hideLinks, setHideLinks] = React.useState(\"show\");\n    const[menuClick, setMenuClick] = React.useState(false);\n    const[showSidebar, setShowSidebar] = React.useState(\"hide\");\n    const{token, setToken, user, setUser} = useContext(TokenContext);\n\n    function handleLogout() {\n        // // fetch('http://127.0.0.1:8000/api/logout/', {\n        // // method: 'GET',\n        // // headers: {\n        // //   \"Content-type\": \"application/json; charset=UTF-8\",\n        //   //'X-CSRFToken': token,\n        // // },\n        // // })\n        // // props.handlePage(\"login\")\n        // // Clear Context variables and local storage\n        setUser(null)\n        setToken(null)\n        localStorage.removeItem(\"userName\")\n        localStorage.removeItem(\"userEmail\")\n        alert(\"You have been logged out\")\n        return <Navigate to=\"/login\"/>\n    }\n    \n    React.useEffect(() => {\n        \n        // Hide or show Nav Links\n        function handleResize() {\n            if(window.innerWidth < 940) {\n                setHideLinks(\"hide\")\n            } else {\n                setHideLinks(\"show\")\n                setShowSidebar(\"hide\")\n                setMenuClick(false)\n            }\n        }\n        \n        // Listen for window size change\n        window.addEventListener(\"resize\", handleResize);\n        handleResize()\n    }, [])\n\n    function handleIconClick() {\n        setShowSidebar(showSidebar === \"hide\" ? \"show\" : \"hide\")\n    }\n\n    return (\n        <>\n            <nav className=\"nav-bar\">\n                <FontAwesomeIcon \n                    icon={faBars} size=\"2xl\" \n                    className={hideLinks === \"show\" ? \"icon-fa-bar-hidden\" : \n                        menuClick === false ? \"icon-fa-bar\" : \"icon-fa-bar-open\"} \n                    onClick={() => {handleIconClick(); setMenuClick(!menuClick)}}\n                    />\n                <h1 className={hideLinks ===\"hide\" ? \"saving-logo-center\" : \"saving-logo\"}>Saving Stars</h1>\n                <MenuListData \n                    className={hideLinks=== \"show\" ? \"nav-list\" : \"nav-list-hidden\"}\n                    handlePage={props.handlePage}\n                    handleLogout={handleLogout}\n                    />\n            </nav>\n            <nav>\n                <Sidebar \n                    className={showSidebar === \"show\" ? \"sidebar-div\" : \"sidebar-div-hidden\"}\n                    handlePage={props.handlePage}\n                    handleClick={() => {handleIconClick(); setMenuClick(false)}}\n                    handleLogout={handleLogout}\n                    />\n            </nav>\n            <Outlet />\n        </>\n    )\n}\n\nexport default Navbar\n","/Users/benhillier/Documents/GitHub/kids-saving-app/savings/frontend/src/components/Login.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app/savings/frontend/src/components/ParentPortal.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app/savings/frontend/src/contexts/Token.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app/savings/frontend/src/components/MenuListData.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app/savings/frontend/src/components/Sidebar.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app/savings/frontend/src/components/CSRFToken.js",["208"],[],"import React from \"react\";\nimport currentToken from \"./currentToken\";\n\n// Get CSRF Token\nfunction getCookie(name) {\n    let cookieValue = null;\n    if (document.cookie && document.cookie !== '') {\n        const cookies = document.cookie.split(';');\n        for (let i = 0; i < cookies.length; i++) {\n            const cookie = cookies[i].trim();\n            // Does this cookie string begin with the name we want?\n            if (cookie.substring(0, name.length + 1) === (name + '=')) {\n                cookieValue = decodeURIComponent(cookie.substring(name.length + 1));\n                break;\n            }\n        }\n    }\n    currentToken(cookieValue)\n    return cookieValue;\n}\nconst csrftoken = getCookie('csrftoken');\n\n\nexport default csrftoken","/Users/benhillier/Documents/GitHub/kids-saving-app/savings/frontend/src/components/currentToken.js",["209"],[],"import React from 'react'\n\nfunction currentToken(token) {\n    const userToken = token\n    return userToken;\n}\n\nexport default currentToken\n","/Users/benhillier/Documents/GitHub/kids-saving-app/savings/frontend/src/components/Tasks.js",["210","211","212","213","214","215","216"],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/index.js",[],[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/App.js",["217","218","219","220"],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/reportWebVitals.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/Navbar.js",["221","222","223","224"],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/Dashboard.js",["225","226"],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/ErrorPage.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/Login.js",["227"],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/ParentPortal.js",["228","229","230"],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/Register.js",[],[],"/Use