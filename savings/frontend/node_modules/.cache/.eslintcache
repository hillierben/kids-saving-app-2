= React.useState()\n\n  return (\n    <TokenContext.Provider value={{token, setToken, user, setUser}}>\n      <BrowserRouter>\n        <Routes>\n          <Route path=\"/\" element={<Navbar \n            handlePage={e => setPageSelector(e)}\n            setLoggedOut={e => setLoggedOut(e)}/>}>\n              <Route path=\"/register\" element={<Register />} />\n              <Route path=\"/login\" element={<Login setPageSelector={e => setPageSelector(e)}/>} />\n              <Route path=\"/dashboard\" element={<Dashboard />} />\n              <Route path=\"/portal\" element={<ParentPortal />} />\n          </Route>\n          <Route path=\"*\" element={<ErrorPage />} />\n        </Routes>\n      </BrowserRouter>\n    </TokenContext.Provider>\n  );\n}\n\nexport default App;\n","/Users/benhillier/Documents/GitHub/kids-saving-app/savings/frontend/src/components/Register.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app/savings/frontend/src/components/Dashboard.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app/savings/frontend/src/components/ErrorPage.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app/savings/frontend/src/components/Navbar.js",["196","197"],[],"import React from \"react\";\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome'\nimport { faBars } from '@fortawesome/free-solid-svg-icons'\nimport MenuListData from \"./MenuListData\";\nimport Sidebar from \"./Sidebar\";\nimport { Outlet } from \"react-router-dom\";\nimport { Navigate } from \"react-router-dom\";\nimport { useContext } from \"react\";\nimport TokenContext from \"../contexts/Token\";\n\nconst Navbar = (props) => {\n    const[hideLinks, setHideLinks] = React.useState(\"show\");\n    const[menuClick, setMenuClick] = React.useState(false);\n    const[showSidebar, setShowSidebar] = React.useState(\"hide\");\n    const{token, setToken, user, setUser} = useContext(TokenContext);\n\n    function handleLogout() {\n        // // fetch('http://127.0.0.1:8000/api/logout/', {\n        // // method: 'GET',\n        // // headers: {\n        // //   \"Content-type\": \"application/json; charset=UTF-8\",\n        //   //'X-CSRFToken': token,\n        // // },\n        // // })\n        // // props.handlePage(\"login\")\n        // // Clear Context variables and local storage\n        setUser(null)\n        setToken(null)\n        localStorage.removeItem(\"userName\")\n        localStorage.removeItem(\"userEmail\")\n        alert(\"You have been logged out\")\n        return <Navigate to=\"/login\"/>\n    }\n    \n    React.useEffect(() => {\n        \n        // Hide or show Nav Links\n        function handleResize() {\n            if(window.innerWidth < 940) {\n                setHideLinks(\"hide\")\n            } else {\n                setHideLinks(\"show\")\n                setShowSidebar(\"hide\")\n                setMenuClick(false)\n            }\n        }\n        \n        // Listen for window size change\n        window.addEventListener(\"resize\", handleResize);\n        handleResize()\n    }, [])\n\n    function handleIconClick() {\n        setShowSidebar(showSidebar === \"hide\" ? \"show\" : \"hide\")\n    }\n\n    return (\n        <>\n            <nav className=\"nav-bar\">\n                <FontAwesomeIcon \n                    icon={faBars} size=\"2xl\" \n                    className={hideLinks === \"show\" ? \"icon-fa-bar-hidden\" : \n                        menuClick === false ? \"icon-fa-bar\" : \"icon-fa-bar-open\"} \n                    onClick={() => {handleIconClick(); setMenuClick(!menuClick)}}\n                    />\n                <h1 className={hideLinks ===\"hide\" ? \"saving-logo-center\" : \"saving-logo\"}>Saving Stars</h1>\n                <MenuListData \n                    className={hideLinks=== \"show\" ? \"nav-list\" : \"nav-list-hidden\"}\n                    handlePage={props.handlePage}\n                    handleLogout={handleLogout}\n                    />\n            </nav>\n            <nav>\n                <Sidebar \n                    className={showSidebar === \"show\" ? \"sidebar-div\" : \"sidebar-div-hidden\"}\n                    handlePage={props.handlePage}\n                    handleClick={() => {handleIconClick(); setMenuClick(false)}}\n                    handleLogout={handleLogout}\n                    />\n            </nav>\n            <Outlet />\n        </>\n    )\n}\n\nexport default Navbar\n","/Users/benhillier/Documents/GitHub/kids-saving-app/savings/frontend/src/components/Login.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app/savings/frontend/src/components/ParentPortal.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app/savings/frontend/src/contexts/Token.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app/savings/frontend/src/components/MenuListData.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app/savings/frontend/src/components/Sidebar.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app/savings/frontend/src/components/CSRFToken.js",["198"],[],"import React from \"react\";\nimport currentToken from \"./currentToken\";\n\n// Get CSRF Token\nfunction getCookie(name) {\n    let cookieValue = null;\n    if (document.cookie && document.cookie !== '') {\n        const cookies = document.cookie.split(';');\n        for (let i = 0; i < cookies.length; i++) {\n            const cookie = cookies[i].trim();\n            // Does this cookie string begin with the name we want?\n            if (cookie.substring(0, name.length + 1) === (name + '=')) {\n                cookieValue = decodeURIComponent(cookie.substring(name.length + 1));\n                break;\n            }\n        }\n    }\n    currentToken(cookieValue)\n    return cookieValue;\n}\nconst csrftoken = getCookie('csrftoken');\n\n\nexport default csrftoken","/Users/benhillier/Documents/GitHub/kids-saving-app/savings/frontend/src/components/currentToken.js",["199"],[],"import React from 'react'\n\nfunction currentToken(token) {\n    const userToken = token\n    return userToken;\n}\n\nexport default currentToken\n","/Users/benhillier/Documents/GitHub/kids-saving-app/savings/frontend/src/components/Tasks.js",["200","201","202","203","204","205","206"],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/index.js",[],[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/App.js",["207","208","209","210"],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/reportWebVitals.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/Navbar.js",["211","212","213","214"],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/Dashboard.js",["215","216","217","218","219"],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/ErrorPage.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/Login.js",["220"],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/ParentPortal.js",["221","222","223"],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/Register.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/contexts/Token.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/Sidebar.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/MenuListData.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/CSRFToken.js",["224"],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/Tasks.js",["225","226","227","228","229","230","231","232","233","234"],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/currentToken.js",["235"],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/subComponents/CompleteTask.js",["236","237","238","239"],[],"import React, { useEffect } from 'react'\nimport { useState } from 'react'\nimport { Popconfirm } from 'antd'\nimport { useContext } from 'react'\nimport TokenContext from '../../contexts/Token'\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome';\nimport { faSquareCheck } from '@fortawesome/free-solid-svg-icons'\nimport Popup from './Popup'\n\n\n\n\nconst CompleteTask = ({task, getTasks}) => {\n    const[completeID, setCompleteID] = useState()\n    const{token} = useContext(TokenContext)\n    const[complete, setComplete] = useState()\n    const[isOpen, setIsOpen] = React.useState(false)\n\n    function handleComplete(e) {\n        let completeTask = async (e)=>{\n            let response = await fetch(`http://127.0.0.1:8000/api/edit-task/${completeID}/`, {\n                method: \"PUT\",\n                headers: {\n                    \"Content-Type\": \"application/json\",\n                    \"Authorization\": \"Bearer \" + localStorage.getItem(\"token\"),\n                },\n                body: JSON.stringify({\n                    \"id\": completeID,\n                    \"task\": task.task,\n                    \"amount\": task.amount,\n                    \"created\": task.created,\n                    \"complete\": complete,\n                    \"user\": task.user,\n                })\n            })\n            setComplete(prev=>(!prev))\n        }\n        completeTask(e)\n    }\n    \n    useEffect(()=>{\n        getTasks()\n    },[complete])\n\n    return (\n        <>\n            <Popup isOpen={isOpen} close={() => setIsOpen(false)} handle={handleComplete}>\n                Are you sure you want to complete this task?\n            </Popup>\n            <FontAwesomeIcon \n                icon={faSquareCheck} \n                className='icon-margin' \n                size=\"xl\"\n                style={{color: \"#333333\",}} \n                onClick={()=>{setCompleteID(task.id); \n                    setComplete(!task.complete); \n                    setIsOpen(true)}} />\n        </>\n    )\n}\n\nexport default CompleteTask\n",[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/subComponents/DeleteTask.js",["240","241","242"],[],"import React from 'react'\nimport { useState } from 'react'\nimport { Popconfirm } from 'antd'\nimport { FontAwesomeIcon } from '@fortawesome/react-fontawesome';\nimport { faTrash } from '@fortawesome/free-solid-svg-icons'\nimport Popup from './Popup';\n\nconst DeleteTask = (props) => {\n    const[deleteID, setDeleteID] = useState()\n    const[isOpen, setIsOpen] = useState(false)\n\n    const POP_STYLE = {\n        backgroundColor: \"red\",\n    }\n\n    let deleteTask = async () => {\n        let taskDelete = deleteID\n        let response = await fetch(`http://127.0.0.1:8000/api/edit-task/${taskDelete}/`, {\n            method: \"POST\",\n            headers: {\n                \"Content-Type\": \"application/json\",\n                \"Authorization\": \"Bearer \" + localStorage.getItem(\"token\")\n            }\n        })\n        let data = await response.json()\n        props.getTasks()\n    }\n\n    function handleDelete(e) {\n           \n        deleteTask()\n    }\n    \n    return (\n        <>\n           <Popup isOpen={isOpen} close={() => setIsOpen(false)} handle={handleDelete}>\n                Are you sure you want to DELETE this task?\n            </Popup>\n            <FontAwesomeIcon \n                icon={faTrash} \n                className='icon-margin'\n                size=\"xl\" \n                name=\"delete\" \n                onClick={() => \n                    {setDeleteID(props.taskID);\n                    setIsOpen(true)}}/>\n\n\n        </>\n    )\n}\n\nexport default DeleteTask\n","/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/subComponents/Popup.j