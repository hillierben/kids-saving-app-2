    }\n\n    function handleDelete(e) {\n           \n        deleteTask()\n    }\n    \n    return (\n        <>\n           <Popup isOpen={isOpen} close={() => setIsOpen(false)} handle={handleDelete}>\n                Are you sure you want to DELETE this task?\n            </Popup>\n            <FontAwesomeIcon \n                icon={faTrash} \n                className='icon-margin'\n                size=\"xl\" \n                name=\"delete\" \n                onClick={() => \n                    {setDeleteID(props.taskID);\n                    setIsOpen(true)}}/>\n\n\n        </>\n    )\n}\n\nexport default DeleteTask\n","/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/subComponents/Popup.js",["252"],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/subComponents/Button.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/subComponents/AddChild.js",["253"],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/subComponents/ChildList.js",["254"],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/subComponents/ChildTasks.js",[],[],"/Users/benhillier/Documents/GitHub/kids-saving-app-2/savings/frontend/src/components/subComponents/TotalSaved.js",["255"],[],{"ruleId":"256","severity":1,"message":"257","line":14,"column":9,"nodeType":"258","messageId":"259","endLine":14,"endColumn":21},{"ruleId":"256","severity":1,"message":"260","line":15,"column":9,"nodeType":"258","messageId":"259","endLine":15,"endColumn":13},{"ruleId":"256","severity":1,"message":"261","line":15,"column":15,"nodeType":"258","messageId":"259","endLine":15,"endColumn":22},{"ruleId":"256","severity":1,"message":"262","line":16,"column":9,"nodeType":"258","messageId":"259","endLine":16,"endColumn":18},{"ruleId":"256","severity":1,"message":"263","line":15,"column":11,"nodeType":"258","messageId":"259","endLin